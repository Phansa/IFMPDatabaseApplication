    </DbProviderFactories>
  </system.data>
    <userSettings>
        <WindowsApplication1.My.MySettings>
            <setting name="Resize" serializeAs="String">
                <value />
            </setting>
            <setting name="DB_Password" serializeAs="String">
                <value />
            </setting>
            <setting name="Admin_Password" serializeAs="String">
                <value />
            </setting>
            <setting name="TableStyle" serializeAs="String">
                <value />
            </setting>
            <setting name="IdThroughForms" serializeAs="String">
                <value>Yes</value>
            </setting>
            <setting name="ViewAll" serializeAs="String">
                <value>False</value>
            </setting>
        </WindowsApplication1.My.MySettings>
    </userSettings>
</configuration>